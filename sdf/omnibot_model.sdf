<?xml version="1.0" ?>
<sdf version="1.9">
  <model name="robot">
    <link name="base_footprint"/>

    <link name="base_link">
      <visual name="base_visual_main">
        <pose>0 0 0.025 0 0 0</pose>
        <geometry>
          <box>
            <size>0.3 0.2 0.005</size>
          </box>
        </geometry>
        <material>
          <ambient>0.8 0.8 0.8 1.0</ambient>
          <diffuse>0.8 0.8 0.8 1.0</diffuse>
        </material>
      </visual>
      <visual name="base_visual_front">
        <pose>0.1 0 0.045 0 0 0</pose>
        <geometry>
          <box>
            <size>0.07 0.076 0.03</size>
          </box>
        </geometry>
      </visual>
      <collision name="base_collision">
        <pose>0 0 0.025 0 0 0</pose>
        <geometry>
          <box>
            <size>0.3 0.2 0.01</size>
          </box>
        </geometry>
      </collision>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>20.0</mass>
        <inertia>
          <ixx>2.133</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>4.266</iyy>
          <iyz>0</iyz>
          <izz>5.546</izz>
        </inertia>
      </inertial>
    </link>

    <joint name="base_joint" type="fixed">
      <parent>base_footprint</parent>
      <child>base_link</child>
      <pose>0 0 0.08 0 0 0</pose>
    </joint>

    <!-- Wheels -->
    <link name="leftfront_link">
      <visual name="leftfront_visual">
        <geometry>
          <mesh>
            <uri>package://ign_robots_examples/meshes/left_mecanum_wheel.dae</uri>
            <scale>1.0 0.8 1.0</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="leftfront_collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>6.1e-5</ixx>
          <iyy>6.1e-5</iyy>
          <izz>6.1e-5</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="leftfront_joint" type="continuous">
      <parent>base_link</parent>
      <child>leftfront_link</child>
      <pose>0.085 0.14 0.01 -1.5708 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <link name="rightfront_link">
      <visual name="rightfront_visual">
        <geometry>
          <mesh>
            <uri>package://ign_robots_examples/meshes/right_mecanum_wheel.dae</uri>
            <scale>1.0 0.8 1.0</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="rightfront_collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>6.1e-5</ixx>
          <iyy>6.1e-5</iyy>
          <izz>6.1e-5</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="rightfront_joint" type="continuous">
      <parent>base_link</parent>
      <child>rightfront_link</child>
      <pose>0.085 -0.14 0.01 -1.5708 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <link name="rightback_link">
      <visual name="rightback_visual">
        <geometry>
          <mesh>
            <uri>package://ign_robots_examples/meshes/left_mecanum_wheel.dae</uri>
            <scale>1.0 0.8 1.0</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="rightback_collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>6.1e-5</ixx>
          <iyy>6.1e-5</iyy>
          <izz>6.1e-5</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="rightback_joint" type="continuous">
      <parent>base_link</parent>
      <child>rightback_link</child>
      <pose>-0.085 -0.14 0.01 -1.5708 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <link name="leftback_link">
      <visual name="leftback_visual">
        <geometry>
          <mesh>
            <uri>package://ign_robots_examples/meshes/right_mecanum_wheel.dae</uri>
            <scale>1.0 0.8 1.0</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="leftback_collision">
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>6.1e-5</ixx>
          <iyy>6.1e-5</iyy>
          <izz>6.1e-5</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="leftback_joint" type="continuous">
      <parent>base_link</parent>
      <child>leftback_link</child>
      <pose>-0.085 0.14 0.01 -1.5708 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <!-- Lidar -->
    <link name="lidar_link">
      <visual name="lidar_visual">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.07</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
        </material>
      </visual>
      <sensor name="gpu_lidar" type="gpu_lidar">
        <pose>0.1 0 0.15 0 0 0</pose>
        <update_rate>10</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>360</samples>
              <resolution>1</resolution>
              <min_angle>-2.1</min_angle>
              <max_angle>2.1</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.1</min>
            <max>40.0</max>
            <resolution>0.017453</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <always_on>true</always_on>
        <visualize>false</visualize>
        <topic>lidar</topic>
      </sensor>
    </link>
    <joint name="lidar_joint" type="fixed">
      <parent>base_link</parent>
      <child>lidar_link</child>
      <pose>0.1 0 0.15 0 0 0</pose>
    </joint>

    <!-- RGBD Camera -->
    <link name="rgbd_frame">
      <visual name="rgbd_visual">
        <geometry>
          <box>
            <size>0.02 0.07 0.03</size>
          </box>
        </geometry>
        <material>
          <ambient>0.9 0.9 0.9 1.0</ambient>
          <diffuse>0.9 0.9 0.9 1.0</diffuse>
        </material>
      </visual>
      <sensor name="rgbd_camera" type="rgbd_camera">
        <pose>0.18 0 0.06 0 0 0</pose>
        <update_rate>10</update_rate>
        <always_on>true</always_on>
        <visualize>false</visualize>
        <topic>depth_camera</topic>
        <camera name="rgbd_frame">
          <horizontal_fov>1.0472</horizontal_fov>
          <lens>
            <intrinsics>
              <fx>277.1</fx>
              <fy>277.1</fy>
              <cx>160.5</cx>
              <cy>120.5</cy>
              <s>0</s>
            </intrinsics>
          </lens>
          <distortion>
            <k1>0.0</k1>
            <k2>0.0</k2>
            <k3>0.0</k3>
            <p1>0.0</p1>
            <p2>0.0</p2>
            <center>0.5 0.5</center>
          </distortion>
          <image>
            <width>320</width>
            <height>240</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.01</near>
            <far>300</far>
          </clip>
          <depth_camera>
            <clip>
              <near>0.1</near>
              <far>10</far>
            </clip>
          </depth_camera>
          <noise>
            <type>gaussian</type>
            <mean>0</mean>
            <stddev>0.004</stddev>
          </noise>
        </camera>
      </sensor>
    </link>
    <joint name="rgbd_joint" type="fixed">
      <parent>base_link</parent>
      <child>rgbd_frame</child>
      <pose>0.18 0 0.06 0 0 0</pose>
    </joint>

    <!-- IMU -->
    <sensor name="imu_sensor" type="imu">
      <pose>0 0 0 0 0 0</pose>
      <always_on>true</always_on>
      <update_rate>50</update_rate>
      <visualize>false</visualize>
      <topic>imu</topic>
      <imu>
        <angular_velocity>
          <x>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.009</stddev>
              <bias_mean>0.00075</bias_mean>
              <bias_stddev>0.005</bias_stddev>
              <dynamic_bias_stddev>0.00002</dynamic_bias_stddev>
              <dynamic_bias_correlation_time>400.0</dynamic_bias_correlation_time>
              <precision>0.00025</precision>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.009</stddev>
              <bias_mean>0.00075</bias_mean>
              <bias_stddev>0.005</bias_stddev>
              <dynamic_bias_stddev>0.00002</dynamic_bias_stddev>
              <dynamic_bias_correlation_time>400.0</dynamic_bias_correlation_time>
              <precision>0.00025</precision>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.009</stddev>
              <bias_mean>0.00075</bias_mean>
              <bias_stddev>0.005</bias_stddev>
              <dynamic_bias_stddev>0.00002</dynamic_bias_stddev>
              <dynamic_bias_correlation_time>400.0</dynamic_bias_correlation_time>
              <precision>0.00025</precision>
            </noise>
          </z>
        </angular_velocity>
        <linear_acceleration>
          <x>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.021</stddev>
              <bias_mean>0.05</bias_mean>
              <bias_stddev>0.0075</bias_stddev>
              <dynamic_bias_stddev>0.000375</dynamic_bias_stddev>
              <dynamic_bias_correlation_time>175.0</dynamic_bias_correlation_time>
              <precision>0.005</precision>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.021</stddev>
              <bias_mean>0.05</bias_mean>
              <bias_stddev>0.0075</bias_stddev>
              <dynamic_bias_stddev>0.000375</dynamic_bias_stddev>
              <dynamic_bias_correlation_time>175.0</dynamic_bias_correlation_time>
              <precision>0.005</precision>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.021</stddev>
              <bias_mean>0.05</bias_mean>
              <bias_stddev>0.0075</bias_stddev>
              <dynamic_bias_stddev>0.000375</dynamic_bias_stddev>
              <dynamic_bias_correlation_time>175.0</dynamic_bias_correlation_time>
              <precision>0.005</precision>
            </noise>
          </z>
        </linear_acceleration>
      </imu>
    </sensor>

    <!-- Plugins -->
    <plugin filename="ignition-gazebo-mecanum-drive-system" name="ignition::gazebo::systems::MecanumDrive">
      <front_left_joint>leftfront_joint</front_left_joint>
      <front_right_joint>rightfront_joint</front_right_joint>
      <back_left_joint>leftback_joint</back_left_joint>
      <back_right_joint>rightback_joint</back_right_joint>
      <wheel_separation>0.085</wheel_separation>
      <wheelbase>0.14</wheelbase>
      <wheel_radius>0.05</wheel_radius>
      <min_acceleration>-5</min_acceleration>
      <max_acceleration>5</max_acceleration>
    </plugin>

    <plugin filename="ignition-gazebo-odometry-publisher-system" name="ignition::gazebo::systems::OdometryPublisher">
      <dimensions>3</dimensions>
      <robot_base_frame>base_footprint</robot_base_frame>
    </plugin>

  </model>
</sdf>

